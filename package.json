{
  "name": "tree-sitter-tiger",
  "version": "0.1.1",
  "description": "tiger grammar for tree-sitter",
  "main": "bindings/node",
  "keywords": [
    "parsing",
    "incremental"
  ],
  "dependencies": {
    "nan": "^2.12.1"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.20.6"
  },
  "scripts": {
    "test": "tree-sitter test"
  },
  "tree-sitter": [
    {
      "scope": "source.tiger",
      "file-types": [
        "tig",
        "tih"
      ],
      "injection-regex": "tiger"
    }
  ]
}
